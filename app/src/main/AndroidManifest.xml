<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pigredorou.jeuxenvisio">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@drawable/icone_carte"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="UnusedAttribute"
        tools:targetApi="m">
        <activity android:name=".JeuEnVisioActivity"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:theme="@style/SettingsTheme" />
        <activity android:name=".JustOneActivity" />
        <activity android:name=".TimelineActivity" />
        <activity android:name=".MajorityActivity" />
        <activity android:name=".TopTenActivity" />
        <activity android:name=".BeloteActivity" />
        <activity android:name=".Gambit7Activity" />
        <activity android:name=".LeRoiDesNainsActivity" />
        <activity android:name=".FiestaDeLosMuertosActivity" />
        <activity android:name=".TheCrewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>